<div class="row">
<div class="col-md-5">
<div class="form">

<%= form_for(@dataset_type) do |f| %>
  <% if @dataset_type.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@dataset_type.errors.count, "error") %> prohibited this dataset_type from being saved:</h2>

      <ul>
      <% @dataset_type.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class-"field form-group">
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="field form-group">
    <%= f.label :num_cols %><br>
    <%= f.number_field :num_cols, class: "form-control" %>
  </div>
  <div class="field form-group">
    <%= f.label :format %><br>
    <%= f.text_field :format, class: "form-control" %>
  </div>
  <div class="field form-group">
    <%= f.label :image_file %><br>
    <%= f.text_field :image_file, class: "form-control" %>
  </div>
  <div class="field form-group">
    <%= f.label :desciption %><br>
    <%= f.text_area :description, cols: 50, rows: 10, class: "form-control"%>
  </div>
  <div class="actions">
    <%= f.submit class: "btn btn-default" %>
  </div>
<% end %>
