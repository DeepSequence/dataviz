<p id="notice"><%= notice %></p>

<h1>Listing Graph Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graph_types.each do |graph_type| %>
      <tr>
        <td><%= graph_type.name %></td>
        <td><%= link_to 'Show', graph_type %></td>
        <td><%= link_to 'Edit', edit_graph_type_path(graph_type) %></td>
        <td><%= link_to 'Destroy', graph_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Graph type', new_graph_type_path %>
