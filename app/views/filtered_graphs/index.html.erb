<p id="notice"><%= notice %></p>

<h1>Listing Filtered Graphs</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Graph type</th>
      <th>Dataset type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filtered_graphs.each do |filtered_graph| %>
      <tr>
        <td><%= filtered_graph.graph_type.name %></td>
        <td><%= filtered_graph.dataset_type.name %></td>
        <td><%= link_to 'Show', filtered_graph %></td>
        <td><%= link_to 'Edit', edit_filtered_graph_path(filtered_graph) %></td>
        <td><%= link_to 'Destroy', filtered_graph, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Filtered graph', new_filtered_graph_path %>
